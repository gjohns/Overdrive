<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.autoperformanceanalyzer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.autoperformanceanalyzer.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.autoperformanceanalyzer.gui.DisplayModeActivity"
            android:label="@string/title_activity_display_mode" >
        </activity>
        <activity
            android:name="com.autoperformanceanalyzer.Run"
            android:label="@string/title_activity_run" >
        </activity>
        <activity
            android:name="com.autoperformanceanalyzer.gui.CheckErroreckError"
            android:label="@string/title_activity_check_error" >
        </activity>
        <activity
          	android:name="com.autoperformanceanalyzer.gui.ClearCode"
            android:label="@string/title_activity_clear_code" >
        </activity>
        <activity
            android:name="com.autoperformanceanalyzer.gui.History"
            android:label="@string/title_activity_history" >
        </activity>
    </application>

</manifest>
